		"@daonuts/token": "^0.0.1",
		"bases": "^0.2.1",
		"bignumber.js": "^9.0.0",
		"bn.js": "^4.11.8",
		"core-js": "^3.1.4",
		"date-fns": "2.0.0-alpha.22",
		"ethers": "^4.0.38",
